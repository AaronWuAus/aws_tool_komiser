{
  "extends": ["next", "next/core-web-vitals", "airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "prettier/prettier": ["warn", {"endOfLine": "auto"}],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ],
    "no-unused-vars": "off"
  }
}
